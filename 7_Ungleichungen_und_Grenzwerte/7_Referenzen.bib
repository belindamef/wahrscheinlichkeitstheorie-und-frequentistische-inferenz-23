@article{schuck_2017,
	title = {A state representation for reinforcement learning and decision-making in the orbitofrontal cortex},
	url = {http://biorxiv.org/lookup/doi/10.1101/210591},
	doi = {10.1101/210591},
	abstract = {Despite decades of research, the exact ways in which the orbitofrontal cortex (OFC) inﬂuences cognitive function have remained mysterious. Anatomically, the OFC is characterized by remarkably broad connectivity to sensory, limbic and subcortical areas, and functional studies have implicated the OFC in a plethora of functions ranging from facial processing to value-guided choice. Notwithstanding such diversity of ﬁndings, much research suggests that one important function of the OFC is to support decision making and reinforcement learning. Here, we describe a novel theory that posits that OFC’s speciﬁc role in decision-making is to provide an up-to-date representation of task-related information, called a state representation. This representation reﬂects a mapping between distinct task states and sensory as well as unobservable information. We summarize evidence supporting the existence of such state representations in rodent and human OFC and argue that forming these state representations provides a crucial scaﬀold that allows animals to eﬃciently perform decision making and reinforcement learning in high-dimensional and partially observable environments. Finally, we argue that our theory oﬀers an integrating framework for linking the diversity of functions ascribed to OFC and is in line with its wide ranging connectivity.},
	language = {en},
	urldate = {2018-05-30},
	author = {Schuck, Nicolas W. and Wilson, Robert C. and Niv, Yael},
	month = nov,
	year = {2017},
	note = {00000
tex.ids: schuck\_state\_2017-1},
	file = {SchuckEtAl2017.pdf:C\:\\Users\\dirko\\Zotero\\storage\\GXJPLQWL\\SchuckEtAl2017.pdf:application/pdf},
}

