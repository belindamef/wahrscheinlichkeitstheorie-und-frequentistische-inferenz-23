	pages = {498--516},
	file = {Ostwald et al. - 2010 - An information theoretic approach to EEG–fMRI inte.pdf:C\:\\Users\\dirko\\Zotero\\storage\\AB8E5PXR\\Ostwald et al. - 2010 - An information theoretic approach to EEG–fMRI inte.pdf:application/pdf},
