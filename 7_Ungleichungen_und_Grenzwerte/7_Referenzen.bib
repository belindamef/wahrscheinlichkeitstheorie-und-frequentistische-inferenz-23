	note = {00640},
	pages = {e21101},
	file = {Tenopir et al. - 2011 - Data Sharing by Scientists Practices and Percepti.PDF:C\:\\Users\\dirko\\Zotero\\storage\\2HP47BNB\\Tenopir et al. - 2011 - Data Sharing by Scientists Practices and Percepti.PDF:application/pdf},
