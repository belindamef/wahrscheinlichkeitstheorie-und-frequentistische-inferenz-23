@article{kenemans_2000,
	title = {On the processing of spatial frequencies as revealed by evoked-potential source modelingq},
	abstract = {Objectives and methods: Visually evoked potentials (VEPs) are known to be sensitive to spatial frequency, especially in the time range between 50 and 100 ms post-stimulus. In two experiments we localized the cortical activity elicited by stimuli of varying spatial frequency in scalp-recorded brain potentials, using multi-electrode recordings and dipole-source analysis.
